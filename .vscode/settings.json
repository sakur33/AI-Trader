{
  "files.eol": "\n",
  "makefile.extensionOutputFolder": "./.vscode",
  "python.analysis.extraPaths": ["./backend"],
  "files.exclude": {
    "**/__pycache__": true,
    "**/.mypy_cache": true,
    "**/.pytest_cache": true
